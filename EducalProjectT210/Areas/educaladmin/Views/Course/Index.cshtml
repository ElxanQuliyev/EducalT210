@model List<Course>
<a class="btn btn-success" href="/EducalAdmin/Course/Create">Create Course</a>
<div class="row">
        @foreach (var item in Model )
        {
            <div class="col-md-6 grid-margin">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">@item.Name</h4>
                  <p class="card-description">
                    @item.Description
                  </p>
                  <div class="card card-inverse-info" id="context-menu-simple">
                    <div class="card-body">
                      <p class="card-text">
                        @item.Category.CategoryName</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            
        }
                               
          </div>

