<div class="row">
        @foreach (var item in Model )
        {
                <div class="col-md-6 grid-margin">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">@item.CategoryName</h4>
                  <p class="card-description">
                    @item.CategoryDescription
                  </p>
                  <div class="card card-inverse-info" id="context-menu-simple">
                    <div class="card-body">
                      <p class="card-text">
                        @item.CategoryIcon</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            
        }
                               
          </div>

